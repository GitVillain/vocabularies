<http://linked.data.gov.au/def/earth-science-data-category> a owl:Ontology .

earthscicat:conceptScheme  a skos:ConceptScheme ;
    skos:prefLabel "Earth Science Data Category"@en ;
    dcterms:created "2019-06-03T12:11:51.537000+10:00"^^xsd:dateTime ;
    skos:definition "Earth science or geoscience includes all fields of natural science related to the planet Earth. This is a branch of science dealing with the physical constitution of the Earth and its atmosphere."@en .

earthscicat:aster-data a skos:Concept ;
    skos:prefLabel "ASTER data"@en ;
    skos:definition "Advanced Spaceborne Thermal Emission and Reflection Radiometer"@en;
    skos:inScheme earthscicat:conceptScheme ;
    skos:topConceptOf earthscicat:conceptScheme .

earthscicat:exploration-geochemistry-data a skos:Concept ;
    skos:prefLabel "Exploration geochemistry data"@en ;
    skos:definition "Geochemistry is the study of the chemical processes which form and shape the Earth. The Queensland exploration geochemistry includes information on surface and drillholes. The data is generated from open file exploration reports submitted by mining companies doing mineral exploration work in Queensland."@en ;
    skos:inScheme earthscicat:conceptScheme ;
    skos:narrower earthscicat:drillhole-geochemistry-data,
        earthscicat:surface-geochemistry-data ;
    skos:topConceptOf earthscicat:conceptScheme .

earthscicat:drillhole-geochemistry-data a skos:Concept ;
    skos:prefLabel "drillhole geochemistry data"@en ;
    skos:broader earthscicat:exploration-geochemistry-data ;
    skos:inScheme earthscicat:conceptScheme .

earthscicat:surface-geochemistry-data a skos:Concept ;
    skos:prefLabel "surface geochemistry data"@en ;
    skos:broader earthscicat:exploration-geochemistry-data ;
    skos:inScheme earthscicat:conceptScheme .

earthscicat:geophysical-data a skos:Concept ;
    skos:prefLabel "Geophysical data"@en ;
    skos:inScheme earthscicat:conceptScheme ;
    skos:narrower earthscicat:deep-seismic-reflection-surveys,
        earthscicat:electromagnetic-data,
        earthscicat:gravity-data,
        earthscicat:magnetic-data,
        earthscicat:magnetotellurics-surveys,
        earthscicat:radiometric-data,
        earthscicat:seismic-data ;
    skos:topConceptOf earthscicat:conceptScheme .

earthscicat:deep-seismic-reflection-surveys a skos:Concept ;
    skos:prefLabel "deep seismic reflection surveys"@en ;
    skos:broader earthscicat:geophysical-data ;
    skos:inScheme earthscicat:conceptScheme .

earthscicat:electromagnetic-data a skos:Concept ;
    skos:prefLabel "electromagnetic data"@en ;
    skos:broader earthscicat:geophysical-data ;
    skos:inScheme earthscicat:conceptScheme .

earthscicat:gravity-data a skos:Concept ;
    skos:prefLabel "gravity data"@en ;
    skos:definition "The measure of small changes in gravity due to changes in the density of the rocks which make up the Earth provides a method which makes it possible to remotely map the distribution of different rock types and the structure of the rocks."@en ;
    skos:broader earthscicat:geophysical-data ;
    skos:inScheme earthscicat:conceptScheme .

earthscicat:magnetic-data a skos:Concept ;
    skos:prefLabel "magnetic data"@en ;
    skos:definition "The magnetic method is a non-invasive geophysical method which ultimately measures the magnetic field associated with magnetic minerals in crustal rocks."@en ;
    skos:broader earthscicat:geophysical-data ;
    skos:inScheme earthscicat:conceptScheme .

earthscicat:magnetotellurics-surveys a skos:Concept ;
    skos:prefLabel "magnetotellurics surveys"@en ;
    skos:definition "Magnetotellurics (MT) is a passive geophysical method which uses natural time variations of the Earth's magnetic and electric fields to measure the electrical resistivity of the sub-surface."@en ;
    skos:broader earthscicat:geophysical-data ;
    skos:inScheme earthscicat:conceptScheme .

earthscicat:radiometric-data a skos:Concept ;
    skos:prefLabel "radiometric data"@en ;
    skos:definition "The radiometric, or gamma-ray spectrometric method is a geophysical process used to estimate concentrations of the radioelements potassium, uranium and thorium by measuring the gamma-rays which the radioactive isotopes of these elements emit during radioactive decay."@en ;
    skos:broader earthscicat:geophysical-data ;
    skos:inScheme earthscicat:conceptScheme .

earthscicat:seismic-data a skos:Concept ;
    skos:prefLabel "seismic data"@en ;
    skos:broader earthscicat:geophysical-data ;
    skos:inScheme earthscicat:conceptScheme .

earthscicat:hyperspectral-data a skos:Concept ;
    skos:prefLabel "Hyperspectral data"@en ;
    skos:definition "Spectral geology is the measurement and analysis of portions of the electromagnetic spectrum to identify spectrally distinct and physically significant features of different rock types and surface materials, their mineralogy and their alteration signatures."@en;
    skos:inScheme earthscicat:conceptScheme ;
    skos:topConceptOf earthscicat:conceptScheme .

