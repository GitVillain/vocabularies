    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/commodity-code/heavy-rare-earth-oxide> ;
    skos:inScheme cdt:conceptScheme ;
    skos:prefLabel "heavy rare earth oxide"@en .

cdt:heliodor a skos:Concept ;
    skos:broader cdt:beryl ;
    skos:definition "Heliodor is the yellow variety of beryl, the 'mother of gemstones'. Heliodor does not include golden colors which are given the apt name of golden beryl."@en ;
    skos:exactMatch <http://resource.geosciml.org/classifier/cgi/commodity-code/heliodor> ;
    skos:inScheme cdt:conceptScheme ;
    skos:prefLabel "heliodor"@en .

cdt:hematite a skos:Concept ;
    skos:altLabel "haematite"@en ;
    skos:broader cdt:industrial-mineral ;
