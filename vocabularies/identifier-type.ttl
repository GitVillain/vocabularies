<http://linked.data.gov.au/def/identifier-type> a owl:Ontology .

idt:conceptScheme a skos:ConceptScheme ;
 dct:created "2020-03-24T14:56:46+10:00"^^xsd:dateTime ;
 dct:creator <http://linked.data.gov.au/org/gsq> ;
 dct:modified "2020-03-24T14:57:04+10:00"^^xsd:dateTime ;
 dct:source "Compiled by the Geological Survey of Queensland." ;
 skos:definition "Types of Identification documents and numbers used in Queensland as acceptable proof of identity for individuals, businesses, and companies."@en ;
 skos:prefLabel "Identifier Type"@en .

<http://linked.data.gov.au/org/gsq>
 a sdo:Organization ;
 sdo:name "Geological Survey of Queensland" ; 
 .

idt:abn a skos:Concept ;
 skos:altLabel "Australian business number"@en ;
 skos:definition "The Australian Business Number (ABN) is a unique number used to identify business names and companies. The Australian Business Register (ABR) provides ABNs. Source:ASIC"@en ;
 skos:inScheme idt:conceptScheme ;
 skos:prefLabel "ABN"@en ;
 skos:topConceptOf idt:conceptScheme .

idt:acn a skos:Concept ;
 skos:altLabel "Australian company number"@en ;
 skos:definition "Every company in Australia is issued with a unique, nine-digit number when registered. This is an Australian Company Number (ACN) and must be displayed on all company documents. Source:ASIC"@en ;
 skos:inScheme idt:conceptScheme ;
 skos:prefLabel "ACN"@en ;
 skos:topConceptOf idt:conceptScheme .

idt:australian-visa a skos:Concept ;
 skos:definition "A conditional permit granted to a foreign national by the Australian Government conveying rights of entry, residence, or work within the country."@en ;
 skos:inScheme idt:conceptScheme ;
 skos:prefLabel "Australian visa"@en ;
 skos:topConceptOf idt:conceptScheme .

idt:birth-certificate a skos:Concept ;
 skos:definition "A vital record that documents the birth of a child. Birth certificates may be the original document certifying the circumstances of the birth or to a certified copy or representation of the ensuing registration of that birth. "@en ;
 skos:inScheme idt:conceptScheme ;
 skos:prefLabel "Birth certificate"@en ;
 skos:topConceptOf idt:conceptScheme .

idt:certificate-of-australian-citizenship a skos:Concept ;
 skos:definition "An Australian citizenship certificate is an official document that shows that a person is an Australian."@en ;
 skos:inScheme idt:conceptScheme ;
 skos:prefLabel "Certificate of Australian citizenship"@en ;
 skos:topConceptOf idt:conceptScheme .

idt:change-of-name-certificate a skos:Concept ;
 skos:definition "A document issued by a a governing body registering a change of name."@en ;
 skos:inScheme idt:conceptScheme ;
 skos:prefLabel "Change of name certificate"@en ;
 skos:topConceptOf idt:conceptScheme .

idt:drivers-license a skos:Concept ;
 skos:altLabel "Drivers Licence"@en ;
 skos:definition "The official permit required for a person to legally drive a motor vehicle in Australia. Issued by the States and Territories of Australia."@en ;
 skos:inScheme idt:conceptScheme ;
 skos:prefLabel "Drivers license"@en ;
 skos:topConceptOf idt:conceptScheme .

idt:marriage-certificate a skos:Concept ;
 skos:definition "An official statement that two people are married. In most jurisdictions, a marriage certificate is issued by a government official only after the civil registration of the marriage. Source:Wikipedia"@en ;
 skos:inScheme idt:conceptScheme ;
 skos:prefLabel "Marriage certificate"@en ;
 skos:topConceptOf idt:conceptScheme .

idt:medicare-card a skos:Concept ;
 skos:definition "A card, issued to Australian citizens and permanent residents and their families. The card or the Medicare number enables the cardholder to receive a rebate of medical expenses under Australia's Medicare system, as well as subsidised medications under the Pharmaceutical Benefits Scheme (PBS). Source:Wikipedia"@en ;
 skos:inScheme idt:conceptScheme ;
 skos:prefLabel "Medicare card"@en ;
 skos:topConceptOf idt:conceptScheme .

idt:passport a skos:Concept ;
 skos:definition "A passport is a basic identity document accepted as valid for crossing international borders, issued by a country's government to its citizens. Source: The New Oxford Companion to Law"@en ;
 skos:inScheme idt:conceptScheme ;
 skos:prefLabel "Passport"@en ;
 skos:topConceptOf idt:conceptScheme .

idt:company-id a skos:Collection ;
 skos:definition "Identification numbers for businesses and companies within Australia."@en ;
 skos:member idt:abn,
 idt:acn ;
 skos:prefLabel "Company ID"@en .

idt:personal-id a skos:Collection ;
 skos:definition "Australian or state-issued documents accepted as prrof-of-identity by the Queensland Government."@en ;
 skos:member idt:australian-visa,
 idt:birth-certificate,
 idt:certificate-of-australian-citizenship,
 idt:change-of-name-certificate,
 idt:drivers-license,
 idt:marriage-certificate,
 idt:medicare-card,
 idt:passport ;
 skos:prefLabel "Personal ID"@en .

idt:primary-documents a skos:Collection ;
 skos:definition "Personal identification documents that act as primary proof-of-identity in the State of Queensland. Identity checks must make up 100 points and include at least one primary document."@en ;
 skos:member idt:birth-certificate,
 idt:drivers-license,
 idt:passport ;
 skos:prefLabel "Primary Documents"@en .

idt:secondary-documents a skos:Collection ;
 skos:definition "Personal identification documents that act as secondary proof-of-identity in the State of Queensland. Identity checks must make up 100 points and include at least one primary document."@en ;
 skos:member idt:australian-visa,
 idt:certificate-of-australian-citizenship,
 idt:change-of-name-certificate,
 idt:marriage-certificate,
 idt:medicare-card ;
 skos:prefLabel "Secondary Documents"@en .